{
  "name": "@svelte-put/preprocess-helpers",
  "version": "0.0.1",
  "description": "utilities & types for building preprocessors",
  "private": true,
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "files": [
    "src",
    "types"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore \"./**/*/*{ts,js,cjs}\"",
    "format": "prettier --check --ignore-path .gitignore \"./**/*.{ts,js,cjs,json}\"",
    "dts": "dts-buddy",
    "prepublishOnly": "pnpm dts"
  },
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-put/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-put/tree/main/packages/preprocessors/auto-slug",
  "devDependencies": {
    "@types/estree": "^1.0.0",
    "@svelte-put/tsconfig": "workspace:*",
    "dts-buddy": "^0.1.7"
  },
  "peerDependencies": {
    "svelte": "^3.55.0 || ^4.0.0"
  }
}
