    "action",
    "intersect",
    "IntersectionObserver",
    "lazy",
    "load",
    "lazyload"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-put/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-put/tree/main/packages/actions/intersect",
  "devDependencies": {
    "@svelte-put/tsconfig": "workspace:*",
    "dts-buddy": "^0.1.7"
  }
