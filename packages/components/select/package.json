{
  "name": "@svelte-put/select",
  "version": "0.1.0",
  "description": "Svelte feature-rich, extensible, type-safe, accessible select component",
  "type": "module",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "exports": {
    "./package.json": "./package.json",
    "./Select.svelte": "./lib/Select.svelte",
    ".": "./lib/index.js"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "Select.svelte": [
        "./lib/Select.svelte.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "!lib/package.json",
    "!lib/CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "sync": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore \"./**/*/*{ts,js,cjs,svelte}\"",
    "format": "prettier --check --ignore-path .gitignore \"./**/*.{ts,js,cjs,json,svelte,css}\"",
    "api:extract": "api-extractor run --local --verbose",
    "api:document": "api-documenter markdown -i api/temp -o api/docs",
    "api": "run-s api:extract api:document"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnphanquang/svelte-put.git"
  },
  "keywords": [
    "svelte",
    "component",
    "select",
    "async",
    "creatable",
    "accessible"
  ],
  "author": "Quang Phan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnphanquang/svelte-put/issues"
  },
  "homepage": "https://github.com/vnphanquang/svelte-put/tree/main/packages/components/select",
  "dependencies": {
    "@svelte-put/clickoutside": "^1.0.4",
    "@svelte-put/shortcut": "^1.0.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.19.22",
    "@microsoft/api-extractor": "^7.33.4",
    "@svelte-put/apirc": "workspace:^0.0.0",
    "@svelte-put/tsconfig": "workspace:^0.0.0",
    "@sveltejs/adapter-auto": "1.0.0-next.84",
    "@sveltejs/kit": "1.0.0-next.524",
    "@sveltejs/package": "1.0.0-next.5",
    "npm-run-all": "^4.1.5",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  },
  "peerDependencies": {
    "svelte": "^3.2.0"
  }
}
