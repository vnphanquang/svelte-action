{
  "name": "@svelte-put/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "code:build": "pnpm code:process && pnpm code:format",
    "code:process": "ts-node ./scripts/convert-code-files.ts",
    "code:format": "prettier --write \"src/**/*.code.js.svelte\"",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --check --ignore-path ../../.prettierignore \"./**/*.{ts,js,cjs,json,svelte,css}\""
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "static-tree": "^1.2.0"
  },
  "devDependencies": {
    "@svelte-put/avatar": "workspace:*",
    "@svelte-put/clickoutside": "workspace:*",
    "@svelte-put/copy": "workspace:*",
    "@svelte-put/intersect": "workspace:*",
    "@svelte-put/modal": "workspace:*",
    "@svelte-put/movable": "workspace:*",
    "@svelte-put/preprocess-auto-slug": "workspace:*",
    "@svelte-put/select": "workspace:*",
    "@svelte-put/shortcut": "workspace:*",
    "@svelte-put/toc": "workspace:*",
    "@sveltejs/adapter-vercel": "1.0.0-next.81",
    "@sveltejs/kit": "1.0.0-next.532",
    "@tailwindcss/typography": "^0.5.7",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vnphanquang/prettierrc": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "cssnano": "^5.1.14",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "highlight.js": "^11.6.0",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "postcss-nesting": "^10.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-highlight": "^6.2.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-preprocess-import-assets": "^0.2.2",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "type": "module"
}
